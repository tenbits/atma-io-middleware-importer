{
    "name": "atma-io-middleware-importer",
    "version": "1.1.31",
    "description": "File importer middleware for Atma.IO and Atma.Toolkit",
    "author": {
        "name": "Alex Kit",
        "email": "alex.kit@atmajs.com"
    },
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "app-bundler"
    },
    "dependencies": {
        "atma-io-middleware-base": "^1.0.35",
        "atma-logger": ">=1.0.0"
    },
    "devDependencies": {
        "@types/node": "^8.0.34",
        "app-bundler": "0.0.35",
        "atma-io": "^0.2.40",
        "atma-loader-ts": "^1.0.0"
    },
    "app-bundler": {
        "file": "src/index.ts",
        "outputMain": "index.js",
        "package": {
            "module": "commonjs",
            "commonjs": {
                "output": "simplified"
            }
        },
        "defaultExtensions": {
            "js": "ts"
        },
        "middlewares": {
            "ts": [
                "atma-loader-ts:read"
            ]
        },
        "dynamicDependencies": [
            "atma-io-middleware-base",
            "atma-logger"
        ]
    },
    "atma": {
        "plugins": [
            "atma-loader-ts"
        ],
        "settings": {
            "atma-loader-ts": {
                "extensions": [
                    "ts",
                    "test"
                ]
            }
        }
    }
}
